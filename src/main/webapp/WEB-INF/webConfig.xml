<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>BCA_AJ_ServletAndJSP</display-name>




	<servlet>
		<servlet-name>ServletConfig</servlet-name>
		<servlet-class>com.unit4.servletconfig.ServletConfigDemo</servlet-class>

		<init-param>
			<param-name>userName</param-name>
			<param-value>Admin</param-value>
		</init-param>
		
		
		<init-param>
			<param-name>userPassword</param-name>
			<param-value>Admin1234</param-value>
		</init-param>
		
		
	</servlet>

	<servlet-mapping>
		<servlet-name>ServletConfig</servlet-name>
		<url-pattern>/servletconifg</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

</web-app>